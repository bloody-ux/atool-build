{
  "extends": "eslint-config-airbnb/base",
  "parser": "babel-eslint",
  "env": {
    "mocha": true
  },
  "rules": {
    "no-console": [0],
    "space-before-function-paren": [2, "never"],
    "max-len": [2, 160, 2],
    "global-require": [0],
    "import/no-dynamic-require": [0],
    "no-param-reassign": [0]
  }
}
